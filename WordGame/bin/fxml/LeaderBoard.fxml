<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.TextFlow?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.LeaderBoardController">
   <children>
      <TextFlow layoutY="65.0" AnchorPane.bottomAnchor="-7.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="65.0">
         <children>
            <TableView fx:id="table" prefHeight="275.0" prefWidth="500.0">
              <columns>
                <TableColumn fx:id="rank" editable="false" prefWidth="75.0" sortable="false" text="RANK" />
                <TableColumn fx:id="uname" prefWidth="75.0" text="NAME" />
                  <TableColumn fx:id="points" prefWidth="75.0" text="SCORE" />
                  <TableColumn fx:id="time" prefWidth="75.0" text="TIME (sec)" />
              </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
         </children>
      </TextFlow>
      <Label alignment="CENTER" layoutX="172.0" layoutY="14.0" prefHeight="38.0" prefWidth="156.0" style="-fx-background-color: royalblue; -fx-border-color: blue;" text="Leaderboard" textFill="WHITE">
